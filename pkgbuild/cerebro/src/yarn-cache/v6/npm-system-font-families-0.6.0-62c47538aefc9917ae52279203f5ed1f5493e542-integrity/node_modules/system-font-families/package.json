    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.7.2",
    "eslint": "^3.14.1",
    "lodash": "^4.17.4",
    "mocha": "^2.4.5",
    "should": "^8.3.0"
  },
  "author": "Ryan Burgett",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rBurgett/system-font-families/issues"
  },
